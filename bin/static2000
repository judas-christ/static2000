#!/usr/bin/env node
'use strict';

var minimist = require('minimist');
var static2000 = require('../index');

var argv = process.argv;
var args = minimist(argv.slice(2));

//remove empty options
for(var key in args) {
  if(!args[key])
    delete args[key];
}
//and positional ones
delete args['_'];

if(!('dest' in args))
  args.dest = 'www';

static2000(args);
